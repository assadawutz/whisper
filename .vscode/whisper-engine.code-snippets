  "Whisper Full Example": {
    "prefix": "wq-full",
    "body": [
      "'use client'",
      "",
      "import { useState } from 'react'",
      "import {",
      "  quickChat,",
      "  createTask,",
      "  showToast,",
      "  getCurrentProvider,",
      "} from '@/engine/utils/engineHelpers'",
      "",
      "export default function ${1:ExampleComponent}() {",
      "  const [input, setInput] = useState('')",
      "  const [response, setResponse] = useState('')",
      "  const [loading, setLoading] = useState(false)",
      "",
      "  const handleSubmit = async (e: React.FormEvent) => {",
      "    e.preventDefault()",
      "    if (!input.trim()) return",
      "",
      "    setLoading(true)",
      "    try {",
      "      const result = await quickChat(input)",
      "      setResponse(result)",
      "",
      "      // Save to memory",
      "      createTask('AI Chat', {",
      "        summary: \`Chatted about: \\${input.slice(0, 50)}\`,",
      "        outcome: 'success',",
      "        tags: ['chat', 'ai'],",
      "      })",
      "",
      "      showToast('Response received!', 'info')",
      "    } catch (error) {",
      "      console.error(error)",
      "      showToast('Error: ' + (error as Error).message, 'error')",
      "    } finally {",
      "      setLoading(false)",
      "    }",
      "  }",
      "",
      "  const provider = getCurrentProvider()",
      "",
      "  return (",
      "    <div className=\"p-6 max-w-2xl mx-auto\">",
      "      <h1 className=\"text-2xl font-bold mb-4\">${2:AI Chat}</h1>",
      "      <p className=\"text-sm text-gray-500 mb-4\">",
      "        Using: {provider.provider} ({provider.model})",
      "      </p>",
      "",
      "      <form onSubmit={handleSubmit} className=\"space-y-4\">",
      "        <textarea",
      "          value={input}",
      "          onChange={(e) => setInput(e.target.value)}",
      "          className=\"w-full p-3 border rounded-lg\"",
      "          rows={4}",
      "          placeholder=\"Ask me anything...\"",
      "        />",
      "        <button",
      "          type=\"submit\"",
      "          disabled={loading || !input.trim()}",
      "          className=\"px-6 py-2 bg-blue-500 text-white rounded-lg disabled:opacity-50\"",
      "        >",
      "          {loading ? 'Thinking...' : 'Send'}",
      "        </button>",
      "      </form>",
      "",
      "      {response && (",
      "        <div className=\"mt-6 p-4 bg-gray-50 rounded-lg\">",
      "          <h2 className=\"font-semibold mb-2\">Response:</h2>",
      "          <p className=\"whitespace-pre-wrap\">{response}</p>",
      "        </div>",
      "      )}",
      "    </div>",
      "  )",
      "}$0",
    ],
    "description": "Full example component with Whisper Engine",
  },
}
