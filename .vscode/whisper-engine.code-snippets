  "Whisper Hook": {
    "prefix": "wq-hook",
    "body": [
      "import { useState, useEffect } from 'react'",
      "import { engineService } from '@/engine/core/engineService'",
      "",
      "export function use${1:HookName}() {",
      "  const [${2:data}, set${2/(.*)/${1:/capitalize}/}] = useState<${3:any}>(null)",
      "  const [loading, setLoading] = useState(true)",
      "  const [error, setError] = useState<Error | null>(null)",
      "",
      "  useEffect(() => {",
      "    const load = async () => {",
      "      try {",
      "        setLoading(true)",
      "        const result = ${4:await engineService.getTaskMemory()}",
      "        set${2/(.*)/${1:/capitalize}/}(result)",
      "      } catch (err) {",
      "        setError(err as Error)",
      "      } finally {",
      "        setLoading(false)",
      "      }",
      "    }",
      "    load()",
      "  }, [])",
      "",
      "  return { ${2:data}, loading, error }",
      "}$0",
    ],
    "description": "Custom hook with Whisper Engine",
  },

